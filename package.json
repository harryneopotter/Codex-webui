{
  "name": "codex-webui",
  "version": "2.0.0",
  "description": "A tiny, dependency-free Web UI that wraps the local OpenAI Codex CLI",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "test": "node --test tests/*.test.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "codex",
    "webui",
    "openai",
    "cli",
    "wrapper",
    "typescript",
    "nodejs",
    "ai",
    "real-time",
    "sse"
  ],
  "author": "HarryNeoPotter",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harryneopotter/Codex-webui.git"
  },
  "homepage": "https://github.com/harryneopotter/Codex-webui#readme",
  "bugs": {
    "url": "https://github.com/harryneopotter/Codex-webui/issues"
  }
}
